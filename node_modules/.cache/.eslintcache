stringify(body)\n            });\n            console.log(response);\n            setData([...data,{type:body.type,total:body.amount,isincome:true,uuid:body.uuid}])\n        } catch (error) {\n            console.log(error.message)\n        }\n\n    }\n    const deleteData = async(uuid) =>{\n\n        try {\n            \n            const deleteTrans = await fetch(`/transactions/${uuid}`,{\n                method:\"DELETE\"\n            });\n\n            console.log(deleteTrans);\n        } catch (error) {\n            console.log(error.message);\n        }\n        const temp = data.filter(e => e.uuid !== uuid)\n        setData(temp);\n    }\n\n\n    return (\n        <DataContext.Provider value={{data,addExpense,addIncome,deleteData}}>\n            {children}\n        </DataContext.Provider>\n    )\n}\n\nexport default DataContextProvider;","/home/mark/Documents/FinanceAssistant/client/src/components/Income.js",[],"/home/mark/Documents/FinanceAssistant/client/src/components/Expense.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/client/src/index.js",[],["154","155"],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/client/src/App.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/client/src/components/Header.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/client/src/components/AddDetail.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/client/src/components/PieChart.js",["156","157"],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/client/src/components/Details.js",["158","159"],"import React,{useContext, useState, useEffect} from 'react'\nimport Income from './Income';\nimport Expense from './Expense';\n\nimport {DataContext} from '../contexts/DataContext';\n\n\n\nexport const Details = () => {\n    \n\n\n\n\n    const {data} = useContext(DataContext);\n\n\n     const expenses = data.filter((e)=>!e.isincome);\n     const incomes = data.filter((e)=