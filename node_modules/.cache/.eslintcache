isincome);\n    let totalExpense = 0;\n    let totalIncome = 0;\n    incomes.forEach(e => totalIncome = parseInt(totalIncome)  + parseInt(e.total) );\n    expenses.forEach(e=> totalExpense = parseInt(totalExpense)  + parseInt(e.total) );\n    let chartData = {};\n  \n    const options = {\n        plugins:{\n            tooltip:{\n                bodyFont:{\n                    size:18\n                }\n            },\n            legend:{\n                labels:{\n                    color: 'white',\n                    font:{\n                        size: 20\n                    }\n                },\n                borderWidth: 0\n            }\n        },\n        elements: {\n          arc: {\n            borderWidth: 2\n          }\n        }\n      };\n\n        chartData = {\n            fontColor:'white',\n            font:{size: 20},\n            labels:[\"Spent\",\"Remaining\"],\n            datasets:[{\n                borderColor:'white',\n                label:'transactions',\n                data:[totalExpense,(totalIncome-totalExpense)],\n                backgroundColor:['#ff0000','#61892f'],\n                \n            }]\n        }\n        if(totalIncome-totalExpense < 0){\n            chartData = {\n                fontColor:'white',\n                font:{size: 20},\n                labels:[\"Spent\",\"Remaining\"],\n                datasets:[{\n                    borderColor:'#474b4f',\n                    label:'transactions',\n                    data:[totalExpense,(0)],\n                    backgroundColor:['#ff0000','#61892f'],\n                    \n                }]\n            }\n\n        }\n    \n   \n    \n    if(totalExpense+totalIncome >0){\n        return (\n                <div className=\"charts\">\n                    <Doughnut\n                    data={chartData}\n                    height={100}\n                    width={100}\n                    options={options}\n                    />\n                </div>\n            )\n\n    }    \n    else{\n        return(\n            <div class=\"empty-graph\">\n                <h5>Add an Income/Expense for a Graph</h5>\n            </div>\n        )\n    }\n    \n   \n}\n\nexport default PieChart;\n\n","/home/mark/Documents/Projects/personalfinance testing/client/src/contexts/DataContext.js",[],"/home/mark/Documents/Projects/personalfinance testing/client/src/components/Income.js",[],"/home/mark/Documents/Projects/personalfinance testing/client/src/components/Expense.js",[],"/home/mark/Documents/Projects/personalfinance testing/client/src/contexts/PortfolioContext.js",[],"/home/mark/Documents/Projects/personalfinance testing/client/src/components/Portfolio.js",[],"/home/mark/Documents/Projects/personalfinance testing/client/src/components/StockQuote.js",[],"/home/mark/Documents/Projects/personalfinance testing/client/src/components/AddStock.js",["224"],"import React,{useState,useContext} from 'react'\nimport {StockContext} from '../contexts/PortfolioContext';\n\n\n\nexport const AddStock = () => {\n    const [symbol,setSymbol] = useState('');\n    const [shares,setShares] = useState(0);\n    \n    const {addStock} = useContext(StockContext);\n\n\n\n\n    const handleSubmit = (e) => {\n       e.preventDefault()\n       addStock(symbol,shares);\n       setSymbol('');\n       e.target.reset();\n    }\n\n    const change = (e) =>{\n\n        setSymbol(e.target.value);\n    }\n\n\n\n\n    return (\n        <div className=\"add add-stock\">\n        <br></br>\n        <hr></hr>\n        <br></br>\n        <form onSubmit={handleSubmit}>\n            \n            <div className=\"add-type\">\n                <label htmlFor=\"text\">Symbol: </label>\n                <input type=\"text\"  value={symbol} required onChange={(e)=>setSymbol(e.target.value)} placeholder=\"Enter Stock Symbol\"></input>\n            </div>\n            <div className=\"add-total\" >\n                <label htmlFor=\"total\"># Shares: </label>\n                 <input type=\"number\"  required onChange={(e)=>setShares(e.target.value)} placeholder=\"Enter # of Shares Owned\"/>\n            </div>\n            <button  type=\"submit\" className=\"btn\">Add</button>\n           \n            \n        \n        </form>\n    </div>\n    )\n}\nexport default AddStock;\n","/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/index.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/App.js",["225"],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/Totals.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/Header.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/Details.js",["226","227"],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/PieChart.js",["228","229"],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/Portfolio.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/AddDetail.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/AddStock.js",["230"],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/contexts/DataContext.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/contexts/PortfolioContext.js",[],"/home/mark/Documents/Projects/personalFinance/FinanceAssistant/personalfinanceClientSide/Finance-Assistant/src/components/Income.js",[],"/home/mark/Docu